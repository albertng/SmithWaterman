NET "PicoFramework/sys_clk_c" TNM_NET = "SYSCLK" ;
NET "PicoFramework/core*/pcie_clocking_i/clk_125" TNM_NET = "CLK_125" ;
NET "PicoFramework/core*/TxOutClk_bufg" TNM_NET = "TXOUTCLKBUFG";
